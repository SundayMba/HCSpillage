@model IEnumerable<DataPresentation>

@{
    Layout = "_Layout1";
    ViewBag.Title = "Device Data";
}




<div class="container " style="margin-top: 50px; ">
    <h4 class="mt-5 text-capitalize text-center">All Device Data</h4>
    <hr class="border border-success border-2" />
    <br />
    <table class="table table-striped table-success">
        <thead>
            <tr>

                <th>device ID</th>
                <th>Location</th>
                <th>Data</th>
                <th>Status</th>
                <th>Date</th>
                <th>Time</th>
                <th>Config</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var data in Model)
            {
                <tr>

                    <td>@data.DeviceId</td>
                    <td>@data.Location</td>
                    <td>@data.Data</td>
                    <td>@data.Status</td>
                    <td>@data.date</td>
                    <td>@data.Time</td>
                    <td>@data.Config</td>
                </tr>
            }
        </tbody>
    </table>
</div>