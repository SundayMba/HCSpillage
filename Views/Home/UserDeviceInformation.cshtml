@model List<DataPresentation>
@inject UserManager<ApplicationUser> userManager

@{
    ViewBag.Title = "User Device Information";
    var userEmail = Model[0];
}




<div class="container " style="margin-top: 100px; margin-bottom: 500px;">
    <h4 class="mt-5 text-capitalize text-center">Data Representation for Device Id: [@userEmail.DeviceId]</h4>
    <hr class="border border-success border-2" />
    <br />
    <table class="table table-striped table-success">
        <thead>
            <tr>

               
                <th>Location</th>
                <th>Data</th>
                <th>Status</th>
                <th>Date</th>
                <th>Time</th>
                <th>Config</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var data in Model)
            {
                <tr>

                    
                    <td>@data.Location</td>
                    <td>@data.Data</td>
                    <td>@data.Status</td>
                    <td>@data.date</td>
                    <td>@data.Time</td>
                    <td>@data.Config</td>
                </tr>
            }
        </tbody>
    </table>
</div>

@section Script
    {
    <script>
        var element = document.getElementsByTagName("footer")[0];
        element.parentNode.removeChild(element);
    </script>
  }